{
  "name": "Exact Fact",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Exact Fact",
    "default_icon": "images/MF_Logo_32.png",
    "default_popup": "misinfoTab.html"
  },
  "icons": {
    "16": "images/MF_Logo_16.png",
    "32": "images/MF_Logo_32.png",
    "48": "images/MF_Logo_48.png",
    "128": "images/MF_Logo_128.png"
  },
  "background": {
    "scripts": [ "background.js" ]
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js", "mark.min.js", "journalists.js" ],
      "css": [ "boxStyling.css" ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}